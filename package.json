{
    "name": "pmac",
    "version": "0.1.4",
    "author": "Max Zakharzhevskiy",
    "description": "Password manager domain credential switch utility",
    "license": "MIT",
    "main": "index.js",
    "keywords": [
        "utility",
        "javascript",
        "typescript"
    ],
    "scripts": {
        "dev": "tsc",
        "build": "rm -rf dist && tsc",
        "makerep": "gluehtml --nofav ./src/reports/index.html",
        "makesk": "node ./src/reports/build-skeleton.js",
        "makebin": "yarn build && pkg -t host -o pmac dist/index.js"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "fast-xml-parser": "^4.0.9",
        "minimist": "^1.2.5",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@types/node": "18.7.14",
        "@types/rimraf": "^3.0.0"
    }
}
