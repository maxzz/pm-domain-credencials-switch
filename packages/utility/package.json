{
    "name": "@pmac/utility",
    "version": "1.0.0",
    "author": "Max Zakharzhevskiy",
    "description": "Password manager domain credential switch utility",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "tsc -w",
        "build": "rm -rf dist && tsc",
        "copy": "xcopy pmac.exe \"./../..\" /Y",
        "makebin": "del pmac.exe && yarn build && pkg -t host -o pmac dist/index.js && yarn copy"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "fast-xml-parser": "^4.0.9",
        "minimist": "^1.2.5",
        "rimraf": "^3.0.2",
        "@pmac/template": "1.0.0"
    },
    "devDependencies": {
        "@pmac/shared-types": "1.0.0",
        "@types/node": "18.7.14",
        "@types/rimraf": "^3.0.0"
    }
}
